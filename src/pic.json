{
  "type": "object",
  "properties": {
    "tag": {
      "type": "string",
      "minLength": 1
    },
    "url": {
      "type": "string",
      "pattern": "^https://.*instagram.com/.*\\.jpg$"
    },
    "location": {
      "type": "object",
      "properties": {
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        },
        "required": [
          "latitude",
          "longitude"
        ]
      }
    },
    "participant": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "tag",
    "url",
    "location",
    "participant"
  ]
}
